<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="com.vaughn.tradingcards.ui.application"
            name="tradingcards">
         <property
               name="appName"
               value="tradingcards">
         </property>
      </product>
   </extension>
   <extension
         id="application"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run
               class="com.vaughn.tradingcards.ui.Application">
         <parameter name="optimize" value="true"/>
         </run>
      </application>
   </extension>
</plugin>
